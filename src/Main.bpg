PROJECTS = Terminal.exe ProductLoader.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Terminal.exe: SmartClient\Terminal.dpr
  $(DCC)

ProductLoader.exe: Loader\ProductLoader.dpr
  $(DCC)


